{
    "gitbook": ">=3.2.0",
    "variables": {
      "django_version": "2.0.6"
    },
    "pdf": {
        "fontSize": 16
    },
    "plugins": [
        "heading-anchors@1.0.3",
        "richquotes@0.0.9",
        "github@2.0.0",
        "language-picker",
        "codeblock-label",
        "sectionx"
    ],
    "pluginsConfig": {
        "richquotes": {
            "default": false
        },
        "github": {
            "url": "https://github.com/anagno/CodingStyle"
        },
        "language-picker": {
            "grid-columns": 3
        },
        "sectionx": {
            "tag": "b"
        }
    }
}
